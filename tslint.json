{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-standard",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "member-access": [true, "no-public"],
    "no-floating-promises": false,
    "object-literal-sort-keys": false,
    "prettier": true
  }
}
