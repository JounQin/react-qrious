{
  "version": 2,
  "name": "@rxts/react",
  "alias": [
    "react-rx.now.sh",
    "rx-react.now.sh"
  ],
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/react-qrcode/*",
      "dest": "react-qrcode/docs/index.html"
    },
    {
      "src": "/react-qrious/*",
      "dest": "react-qrious/docs/index.html"
    },
    {
      "src": "/*",
      "dest": "react-rx/docs/index.html"
    }
  ]
}
